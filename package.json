{
  "name": "photogram",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Anion11/diplom"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "tsc -b && vite build",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix && npm run prettier:fix",
    "stylelint": "stylelint '**/*.{css,scss}'",
    "stylelint:fix": "stylelint '**/*.{css,scss}' --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "preview": "vite preview",
    "prepare": "husky",
    "format": "yarn tsc && eslint src --ext ts,tsx --report-unused-disable-directives --fix && prettier --write './**/*.{js,jsx,ts,tsx,scss,md,json}' --config ./.prettierrc"
  },
  "dependencies": {
    "@types/web": "^0.0.159",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "path": "^0.12.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.53.2",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^7.2.0",
    "react-router-hash-link": "^2.4.3",
    "swiper": "^11.2.1",
    "vite-plugin-svgr": "^4.2.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@eslint/js": "9.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-input-mask": "^3.0.6",
    "@types/react-router-hash-link": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitejs/plugin-react": "4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "15.6.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "sass": "^1.80.4",
    "stylelint": "^16.1.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-scss": "^6.8.1",
    "typescript": "5.2.2",
    "typescript-eslint": "8.0.1",
    "vite": "5.4.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-sass-glob-import": "^4.0.0",
    "vite-plugin-stylelint": "^6.0.0-beta.1"
  }
}
